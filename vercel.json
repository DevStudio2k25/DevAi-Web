{
  "version": 2,
  "buildCommand": "flutter build web --release",
  "outputDirectory": "build/web",
  "public": true,
  "cleanUrls": true,
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "continue": true
    },
    {
      "src": "/flutter.js",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "continue": true
    },
    {
      "src": "/main.dart.js",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "continue": true
    },
    {
      "src": "/manifest.json",
      "headers": { "cache-control": "public, max-age=0, must-revalidate" }
    },
    {
      "src": "/(.+)",
      "headers": { "cache-control": "public, max-age=0, must-revalidate" }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 